<div class="popover {{class}}">
    <div class="arrow"></div>
    <div class="popover-content">
        <div></div>
    </div>
    <div class="emoji-popover-category-tabs">
        {{#each categories}}
        <span class="emoji-popover-tab-item {{# if @first}} active {{/if}}" data-tab-name='{{name}}'><i class="fa {{icon}}"></i></span>
        {{/each}}
    </div>
</div>
